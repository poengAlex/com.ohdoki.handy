  "flow": {
    "triggers": [
      {
        "id": "handy-offline",
        "title": {
          "en": "Handy offline"
        },
        "titleFormatted": {
          "en": "Handy offline"
        },
        "hint": {
          "en": "Triggers when your Handy is offline"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          }
        ]
      },
      {
        "id": "handy-online",
        "title": {
          "en": "Handy online"
        },
        "titleFormatted": {
          "en": "Handy online"
        },
        "hint": {
          "en": "Triggers when your Handy is online"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          }
        ]
      }
    ],
    "conditions": [
      {
        "id": "handy-connected",
        "title": {
          "en": "Online"
        },
        "titleFormatted": {
          "en": "Online"
        },
        "hint": {
          "en": "Is your Handy online?"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "hamp-start",
        "title": {
          "en": "Alternating motion"
        },
        "titleFormatted": {
          "en": "Starts/Stops ([[start]]) Alternating motion"
        },
        "hint": {
          "en": "Start or stop Handy Alternate Motion Protocol (HAMP)"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "checkbox",
            "name": "start",
            "title": {
              "en": "Start"
            }
          }
        ]
      },
      {
        "id": "hamp-velocity",
        "title": {
          "en": "Alternating motion velocity"
        },
        "titleFormatted": {
          "en": "Sets the HAMP velcoity to [[velocity]]"
        },
        "hint": {
          "en": "Set the speed of HAMP mode"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "range",
            "name": "velocity",
            "title": {
              "en": "Velocity"
            },
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "%"
          }
        ]
      },
      {
        "id": "play-script-preset",
        "title": {
          "en": "Play a script preset"
        },
        "titleFormatted": {
          "en": "Plays a preset CSV file. Preset script [[preset]]."
        },
        "hint": {
          "en": "Play a preset script on Handy"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "dropdown",
            "name": "preset",
            "title": {
              "en": "Preset"
            },
            "values": [
              {
                "id": "up-down-5s-192mms",
                "label": {
                  "en": "Up down for 5 sec medium speed"
                }
              },
              {
                "id": "sinus-900ms",
                "label": {
                  "en": "sinus wave"
                }
              },
              {
                "id": "increasing-10s",
                "label": {
                  "en": "Slow -> medium -> fast (10 sec)"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "play-script",
        "title": {
          "en": "Play a script"
        },
        "titleFormatted": {
          "en": "Plays a CSV file from url: [[url]]"
        },
        "hint": {
          "en": "PLay a script on the Handy"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "text",
            "title": {
              "en": "url"
            },
            "name": "url",
            "placeholder": {
              "en": "The url of the script [CSV]"
            }
          }
        ]
      },
      {
        "id": "set-position",
        "title": {
          "en": "Set position of the slider"
        },
        "titleFormatted": {
          "en": "Set the position of the slider to [[position]] the move should take [[duration]]"
        },
        "hint": {
          "en": "Set the position of the slider"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "range",
            "name": "duration",
            "title": "The duration of the move",
            "label": "ms",
            "min": 1,
            "max": 5000,
            "step": 1
          },
          {
            "type": "range",
            "name": "position",
            "title": "The position of the slider",
            "label": "mm",
            "min": 0,
            "max": 100,
            "step": 1
          }
        ]
      },
      {
        "id": "stroke-zone",
        "title": {
          "en": "Set the stroke zone"
        },
        "titleFormatted": {
          "en": "Set the stroke zone fom [[min]] to [[max]]"
        },
        "hint": {
          "en": "Set the stroke zone"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=handy-masturbator"
          },
          {
            "type": "range",
            "title": {
              "en": "range min"
            },
            "name": "min",
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "%"
          },
          {
            "type": "range",
            "title": {
              "en": "range max"
            },
            "name": "max",
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "%"
          }
        ]
      }
    ]
  },
